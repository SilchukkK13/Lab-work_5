    #include <stdio.h>
    #include <stdlib.h>
    #include <time.h>
    #include <conio.h>
    int main ()
  {    int n;
    printf ("\n\t\tGenerating random numbers\n\n\n");
    printf ("Enter the number of sequence numbers:  n = ");
    scanf ("%d", &n);
    srand (time(NULL) ); 
	printf("\n\n\tRandom integers in the interval [13,900]:\n");
    for (int i=0; i<n; i++)
   {     if (i%10==0) printf("\n");
    int number = rand()%900-13;
    printf("%7d", number);
}
return 0;
}
